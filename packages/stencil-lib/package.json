{
  "name": "@example/stencil-lib",
  "version": "0.0.1",
  "description": "Stencil Component Library","type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.js"
    },
    "./hydrate": {
      "types": "./hydrate/index.d.ts",
      "import": "./hydrate/index.mjs",
      "require": "./hydrate/index.js",
      "default": "./hydrate/index.mjs"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.js",
      "default": "./loader/index.js"
    },
    "./components/*.js": {
      "types": "./dist/types/components/*.d.ts",
      "import": "./components/*.js"
    },
    "./components": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --prod",
    "start": "stencil build --watch --prod",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "latest",
    "@stencil/react-output-target": "latest"
  }
}
